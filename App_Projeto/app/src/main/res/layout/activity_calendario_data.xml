<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vistas.CalendarioData">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="160dp"
        android:layout_height="138dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="135dp"
        app:srcCompat="@drawable/avatar_example" />

    <TextView
        android:id="@+id/tvCalendarioData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView10"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="-50dp"
        android:fontFamily="@font/bold"
        android:text="01/01/1990"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/tvCalendarioData2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView10"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_marginStart="-113dp"
        android:layout_marginBottom="-146dp"
        android:fontFamily="@font/bold"
        android:text="Aula de Natação - 18:00h"
        android:textSize="18sp" />

</RelativeLayout>